{
    "name": "smartemis",
    "version": "0.0.1",
    "author": "Ionic Framework",
    "homepage": "https://ionicframework.com/",
    "scripts": {
        "ng": "ng",
        "start": "ng serve", 
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint && npm run stylelint",
        "android:watch": "ionic cap run android -l --external",
        "ios:watch": "ionic cap run ios -l --external",
        "ios": "ionic cap run ios",
        "android": "ionic cap run android",
        "e2e": "ng e2e",
        "stylelint": "stylelint \"src/**/*.scss\"",
        "prettier:check": "prettier --list-different './src/**/*.ts'"
    },
    "lint-staged": {
        "*.scss": [
            "node_modules/.bin/prettier --write --config .prettierrc",
            "stylelint",
            "git add"
        ],
        "*.ts": [
            "node_modules/.bin/prettier --write --config .prettierrc",
            "tslint",
            "git add"
        ],
        "*.html": [
            "node_modules/.bin/js-beautify -r",
            "git add"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/common": "~8.2.14",
        "@angular/core": "~8.2.14",
        "@angular/forms": "~8.2.14",
        "@angular/platform-browser": "~8.2.14",
        "@angular/platform-browser-dynamic": "~8.2.14",
        "@angular/router": "~8.2.14",
        "@capacitor/android": "^1.5.1",
        "@capacitor/core": "1.5.1",
        "@capacitor/ios": "^1.5.1",
        "@ionic-native/core": "^5.0.0",
        "@ionic-native/splash-screen": "^5.0.0",
        "@ionic-native/status-bar": "^5.0.0",
        "@ionic/angular": "^5.0.0",
        "core-js": "^2.5.4",
        "rxjs": "~6.5.1",
        "tslib": "^1.9.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.20",
        "@angular/cli": "~8.3.23",
        "@angular/compiler": "~8.2.14",
        "@angular/compiler-cli": "~8.2.14",
        "@angular/language-service": "~8.2.14",
        "@capacitor/cli": "1.5.1",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@ionic/angular-toolkit": "^2.1.1",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "husky": "^4.2.3",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "js-beautify": "^1.10.3",
        "karma": "~6.4.2",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "lint-staged": "^10.0.8",
        "prettier": "^1.19.1",
        "protractor": "~5.4.4",
        "stylelint": "^13.2.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-scss": "^3.14.2",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "~3.4.3"
    },
    "description": "An Ionic project",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    }
}